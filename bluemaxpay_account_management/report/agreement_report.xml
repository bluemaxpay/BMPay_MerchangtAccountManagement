<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="report_action_agreement_bluemaxpay" model="ir.actions.report">
            <field name="name">Propay Agreement</field>
            <field name="model">bluemaxpay.agreement</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">bluemaxpay_account_management.report_agreement_bluemaxpay</field>
            <field name="report_file">bluemaxpay_account_management.report_agreement_bluemaxpay</field>
            <field name="print_report_name">'Agreement Bluemaxpay'</field>
            <field name="binding_model_id" ref="model_bluemaxpay_agreement"/>
            <field name="binding_type">report</field>
        </record>

        <template id="report_agreement_bluemaxpay">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="web.external_layout">
                        <center><t t-raw="doc.heading"/></center>
                        <t t-raw="doc.description"/>
                    </t>
                </t>
            </t>
        </template>

    </data>
</odoo>